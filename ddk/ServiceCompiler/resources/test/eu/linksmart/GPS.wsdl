<?xml version="1.0" encoding="utf-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:impl="http://linksmart.eu" targetNamespace="http://linksmart.eu">

<message name="sendPositionRequest">
	<part name="requestID" type="xs:string"/>
</message>

<message name="sendPositionResponse">
<part name="position" type="xs:string"/>
</message>

<portType name="GPSServicePort">
  <operation name="getGPSPosition">
      <input message="impl:sendPositionRequest"/>
      <output message="impl:sendPositionResponse"/>
  </operation>
</portType>

<binding name="GPSServiceBinding" type="impl:GPSServicePort">
	<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
	<operation name="getGPSPosition">
		<soap:operation soapAction="getGPSPosition" style="rpc"/>
		<input name="sendPositionRequest">
			<soap:body use="literal"/>
		</input>
		<output name="sendPositionResponse">
			<soap:body use="literal"/>
		</output>
	</operation>
</binding>

<service name="GPSService">
	<port name="GPSServicePort" binding="impl:GPSServiceBinding">
		<soap:address location="http://linksmart.eu/GPS"/>
	</port>
</service>	
</definitions>

