      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="file:./resources/Error.owl#DeviceError"/>
          <owl:Class rdf:about="file:./resources/Error.owl#DeviceWarning"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
  <owl:FunctionalProperty rdf:ID="warningName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="file:./resources/Error.owl#DeviceWarning"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="remedyname">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="file:./resources/Error.owl#Remedy"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="causeName">
