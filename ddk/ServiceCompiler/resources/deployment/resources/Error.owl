    <rdfs:domain rdf:resource="file:./resources/Error.owl#Cause"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="errorName">
    <rdfs:domain rdf:resource="file:./resources/Error.owl#DeviceError"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
