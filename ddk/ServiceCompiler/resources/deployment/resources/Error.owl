  </Cause>
  <DeviceError rdf:ID="PumpFailedToStart">
    <hasCause rdf:resource="file:./resources/Error.owl#PowerSupplyFailure"/>
    <hasRemedy>
      <Remedy rdf:ID="checkPowerSupply">
        <remedyname rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
