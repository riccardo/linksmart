    <rdfs:range rdf:resource="file:./resources/Error.owl#Cause"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasRemedy">
    <rdfs:range rdf:resource="file:./resources/Error.owl#Remedy"/>
