  </owl:Class>
  <owl:Class rdf:ID="Scaledown">
    <rdfs:subClassOf rdf:resource="file:./resources/Error.owl#DeviceWarning"/>
  </owl:Class>
  <owl:Class rdf:ID="Cause">
    <rdfs:subClassOf rdf:resource="file:./resources/Error.owl#Malfunction"/>
