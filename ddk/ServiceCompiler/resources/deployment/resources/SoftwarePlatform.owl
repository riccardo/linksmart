    ></rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="file:./resources/Java.owl#JavaByteCode">
    <rdfs:subClassOf rdf:resource="file:./resources/SoftwarePlatform.owl#ByteCode"/>
