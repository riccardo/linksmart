  </owl:Class>
  <owl:Class rdf:about="file:./resources/Java.owl#JavaLibrary">
    <rdfs:subClassOf rdf:resource="file:./resources/SoftwarePlatform.owl#Library"/>
  </owl:Class>
  <owl:Class rdf:about="file:./resources/Java.owl#JavaVM">
    <rdfs:subClassOf>
      <owl:Class rdf:about="file:./resources/SoftwarePlatform.owl#VirtualMachine"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="file:./resources/Java.owl#JavaByteCode"/>
            <owl:onProperty rdf:resource="file:./resources/SoftwarePlatform.owl#runs"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:allValuesFrom rdf:resource="file:./resources/Java.owl#JavaByteCode"/>
            <owl:onProperty rdf:resource="file:./resources/SoftwarePlatform.owl#runs"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="SuperWaba">
    <rdfs:subClassOf rdf:resource="file:./resources/SoftwarePlatform.owl#Library"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="file:./resources/Java.owl#JavaByteCode"/>
        <owl:onProperty rdf:resource="file:./resources/SoftwarePlatform.owl#runs"/>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="file:./resources/SoftwarePlatform.owl#VirtualMachine">
    <rdfs:subClassOf rdf:resource="file:./resources/SoftwarePlatform.owl#Software"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasOperatingSystem">
