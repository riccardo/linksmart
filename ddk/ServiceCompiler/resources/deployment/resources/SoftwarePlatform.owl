    </rdfs:subPropertyOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasVirtualMachine">
    <rdfs:range rdf:resource="file:./resources/SoftwarePlatform.owl#VirtualMachine"/>
    <rdfs:domain rdf:resource="file:./resources/SoftwarePlatform.owl#SoftwarePlatform"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasLibrary">
    <rdfs:domain rdf:resource="file:./resources/SoftwarePlatform.owl#SoftwarePlatform"/>
    <rdfs:range rdf:resource="file:./resources/SoftwarePlatform.owl#Library"/>
  </owl:ObjectProperty>
  <owl:TransitiveProperty rdf:about="file:./resources/SoftwarePlatform.owl#requires">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:ID="requiresFasterCPU">
    <owl:inverseOf>
      <owl:TransitiveProperty rdf:ID="requiresSlowerCPU"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:ID="requiresMoreMemory">
    <owl:inverseOf>
      <owl:TransitiveProperty rdf:ID="requiresFewerMemory"/>
    </owl:inverseOf>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="file:./resources/SoftwarePlatform.owl#requiresSlowerCPU">
    <owl:inverseOf rdf:resource="file:./resources/SoftwarePlatform.owl#requiresFasterCPU"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="file:./resources/SoftwarePlatform.owl#requiresFewerMemory">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="file:./resources/SoftwarePlatform.owl#requiresMoreMemory"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:ID="provides">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="file:./resources/SoftwarePlatform.owl#Library"/>
    <owl:inverseOf>
      <owl:TransitiveProperty rdf:ID="providedBy"/>
    </owl:inverseOf>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="file:./resources/SoftwarePlatform.owl#providedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="file:./resources/SoftwarePlatform.owl#provides"/>
    <rdfs:domain rdf:resource="file:./resources/SoftwarePlatform.owl#Library"/>
  </owl:TransitiveProperty>
  <owl:FunctionalProperty rdf:ID="versionNumber">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="file:./resources/SoftwarePlatform.owl#Library"/>
          <owl:Class rdf:about="file:./resources/SoftwarePlatform.owl#VirtualMachine"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <SoftwarePlatform rdf:ID="SoftwarePlatform_MPx220">
    <hasVirtualMachine rdf:resource="file:./resources/Java.owl#MIDP2_1"/>
    <hasOperatingSystem rdf:resource="file:./resources/OperatingSystem.owl#WindowsMobileSmartphone_2003"/>
  </SoftwarePlatform>
  <SoftwarePlatform rdf:ID="SoftwarePlatform_N80">
    <hasVirtualMachine rdf:resource="file:./resources/Java.owl#MIDP2_2"/>
    <hasOperatingSystem rdf:resource="file:./resources/OperatingSystem.owl#Symbian_N80"/>
  </SoftwarePlatform>
  <LeJOS rdf:ID="LeJOS_1">
    <versionNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >0.4</versionNumber>
  </LeJOS>
</rdf:RDF>

