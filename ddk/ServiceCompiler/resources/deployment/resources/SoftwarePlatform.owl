    xmlns="file:./resources/SoftwarePlatform.owl#"
    xmlns:Java="file:./resources/Java.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:owl11="http://www.w3.org/2006/12/owl11#"
    xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
    xmlns:DotNet="file:./resources/DotNet.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="file:./resources/SoftwarePlatform.owl">
  <owl:Ontology rdf:about="file:./resources/SoftwarePlatform.owl">
    <owl:imports rdf:resource="file:./resources/DotNet.owl"/>
    <owl:imports rdf:resource="file:./resources/Java.owl"/>
    <owl:imports rdf:resource="file:./resources/OperatingSystem.owl"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Software Platform ontology concepts and definitions</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="LeJOS">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="file:./resources/Java.owl#JavaByteCode"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="runs"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Library"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ByteCode">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Software"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="file:./resources/SoftwarePlatform.owl#Library">
    <rdfs:subClassOf rdf:resource="file:./resources/SoftwarePlatform.owl#Software"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >including library from certain company or device producer</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="PackagingManager">
    <rdfs:subClassOf rdf:resource="file:./resources/SoftwarePlatform.owl#Software"/>
