      <rdf:Description rdf:ID="SoftwarePlatform">
        <rdfs:subClassOf rdf:resource="file:./resources/SoftwarePlatform.owl#Software"/>
      </rdf:Description>
    </rdfs:domain>
    <rdfs:range rdf:resource="file:./resources/OperatingSystem.owl#OperatingSystem"/>
