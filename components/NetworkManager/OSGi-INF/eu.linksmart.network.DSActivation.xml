 <reference bind="cryptoBind" cardinality="1..1" interface="eu.linksmart.security.cryptomanager.CryptoManager" name="CryptoManager" policy="static" unbind="cryptoUnbind"/>
 <reference bind="bindTrustManager" cardinality="0..1" interface="eu.linksmart.security.trustmanager.TrustManager" name="TrustManager" policy="dynamic" unbind="unbindTrustManager"/>
 <reference bind="bindInsideSecurity" cardinality="0..1" interface="eu.linksmart.security.communication.inside.InsideHydra" name="InsideHydra" policy="dynamic" unbind="unbindInsideSecurity"/>
 <reference cardinality="1..1" interface="org.osgi.service.cm.ConfigurationAdmin" name="ConfigurationAdmin" policy="static"/>
 <reference cardinality="1..1" interface="org.osgi.service.http.HttpService" name="HttpService" policy="static"/>
</scr:component>
