<?xml version="1.0"?>
<rdf:RDF
    xmlns:rule="http://localhost/ontologies/PIDRuleModel.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:property="http://localhost/ontologies/StaticPropertyModel.owl#"
    xmlns="http://localhost/ontologies/Application.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:device="http://localhost/ontologies/Device.owl#"
  xml:base="http://localhost/ontologies/Application.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://localhost/ontologies/PIDRuleModel.owl"/>
    <owl:imports rdf:resource="http://localhost/ontologies/StaticPropertyModel.owl"/>
    <owl:imports rdf:resource="http://localhost/ontologies/Device.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="LivingRoom">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Location"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Person">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="BedRoom">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Location"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Location">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Resident">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>
  <owl:Class rdf:ID="SmartHome">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Application"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Application">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Hall">
    <rdfs:subClassOf rdf:resource="#Location"/>
  </owl:Class>
  <owl:Class rdf:ID="MaintananceCrew">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="locatedIn">
    <rdfs:domain rdf:resource="http://localhost/ontologies/Device.owl#Device"/>
    <rdfs:range rdf:resource="#Location"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="ownedBy">
    <rdfs:domain rdf:resource="http://localhost/ontologies/Device.owl#Device"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="personName">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="locationName">
    <rdfs:domain rdf:resource="#Location"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <Application rdf:ID="MyApp1">
    <hasUser>
      <MaintananceCrew rdf:ID="MaintananceCrew_1">
        <ownsDevice>
          <rule:DevicePID rdf:ID="DevicePID_6">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid2</rule:PID>
          </rule:DevicePID>
        </ownsDevice>
        <personName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >myMaintainer1</personName>
      </MaintananceCrew>
    </hasUser>
    <hasUser>
      <Resident rdf:ID="Resident_1">
        <ownsDevice>
          <rule:DevicePID rdf:ID="DevicePID_5">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid2</rule:PID>
          </rule:DevicePID>
        </ownsDevice>
        <personName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >myResident1</personName>
      </Resident>
    </hasUser>
    <hasLocation>
      <BedRoom rdf:ID="BedRoom_1">
        <containsDevice>
          <rule:DevicePID rdf:ID="DevicePID_4">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pidx</rule:PID>
          </rule:DevicePID>
        </containsDevice>
        <containsDevice>
          <rule:DevicePID rdf:ID="DevicePID_3">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid1</rule:PID>
          </rule:DevicePID>
        </containsDevice>
        <locationName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >myBedRoom1</locationName>
      </BedRoom>
    </hasLocation>
    <hasLocation>
      <LivingRoom rdf:ID="LivingRoom_1">
        <containsDevice>
          <rule:DevicePID rdf:ID="DevicePID_2">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid2</rule:PID>
          </rule:DevicePID>
        </containsDevice>
        <containsDevice>
          <rule:DevicePID rdf:ID="DevicePID_1">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid1</rule:PID>
          </rule:DevicePID>
        </containsDevice>
        <locationName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >myLivingRoom1</locationName>
      </LivingRoom>
    </hasLocation>
  </Application>
  <property:MultiValueFormProperty rdf:ID="hasUser">
    <rdfs:range rdf:resource="#Person"/>
    <rdfs:domain rdf:resource="#Application"/>
  </property:MultiValueFormProperty>
  <property:MultiValueFormProperty rdf:ID="ownsDevice">
    <rdfs:range rdf:resource="http://localhost/ontologies/PIDRuleModel.owl#DevicePID"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rule:generatesInverseProperty rdf:resource="#ownedBy"/>
  </property:MultiValueFormProperty>
  <Application rdf:ID="MyApp2">
    <hasUser>
      <Resident rdf:ID="Resident_2">
        <ownsDevice>
          <rule:DevicePID rdf:ID="DevicePID_10">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid3</rule:PID>
          </rule:DevicePID>
        </ownsDevice>
        <ownsDevice>
          <rule:DevicePID rdf:ID="DevicePID_9">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid2</rule:PID>
          </rule:DevicePID>
        </ownsDevice>
        <ownsDevice>
          <rule:DevicePID rdf:ID="DevicePID_8">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid1</rule:PID>
          </rule:DevicePID>
        </ownsDevice>
        <personName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >myResident2</personName>
      </Resident>
    </hasUser>
    <hasLocation>
      <Hall rdf:ID="Hall_1">
        <locationName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >myHall1</locationName>
        <containsDevice>
          <rule:DevicePID rdf:ID="DevicePID_7">
            <rule:PID rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >pid1</rule:PID>
          </rule:DevicePID>
        </containsDevice>
      </Hall>
    </hasLocation>
  </Application>
  <property:MultiValueFormProperty rdf:ID="hasLocation">
    <rdfs:domain rdf:resource="#Application"/>
    <rdfs:range rdf:resource="#Location"/>
  </property:MultiValueFormProperty>
  <property:MultiValueFormProperty rdf:ID="containsDevice">
    <rdfs:range rdf:resource="http://localhost/ontologies/PIDRuleModel.owl#DevicePID"/>
    <rdfs:domain rdf:resource="#Location"/>
    <rule:generatesInverseProperty rdf:resource="#locatedIn"/>
  </property:MultiValueFormProperty>
</rdf:RDF>

