<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:property="http://localhost/ontologies/StaticPropertyModel.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://localhost/ontologies/Hardware.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:unit="http://localhost/ontologies/Unit.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://localhost/ontologies/Hardware.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://localhost/ontologies/Unit.owl"/>
    <owl:imports rdf:resource="http://localhost/ontologies/StaticPropertyModel.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Speaker">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Hardware">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Display">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <property:SingleValueFormProperty rdf:ID="screenHeight">
    <rdfs:range rdf:resource="http://localhost/ontologies/Unit.owl#UnitValue"/>
    <rdfs:domain rdf:resource="#Display"/>
  </property:SingleValueFormProperty>
  <property:MultiValueFormProperty rdf:ID="hasSpeaker">
    <rdfs:domain rdf:resource="#Hardware"/>
    <rdfs:range rdf:resource="#Speaker"/>
  </property:MultiValueFormProperty>
  <property:SingleValueFormProperty rdf:ID="maxVolumeLevel">
    <rdfs:domain rdf:resource="#Speaker"/>
    <rdfs:range rdf:resource="http://localhost/ontologies/Unit.owl#UnitValue"/>
  </property:SingleValueFormProperty>
  <property:MultiValueFormProperty rdf:ID="hasDisplay">
    <rdfs:range rdf:resource="#Display"/>
    <rdfs:domain rdf:resource="#Hardware"/>
  </property:MultiValueFormProperty>
  <property:SingleValueFormProperty rdf:ID="resolution">
    <rdfs:domain rdf:resource="#Display"/>
    <rdfs:range rdf:resource="http://localhost/ontologies/Unit.owl#UnitValue"/>
  </property:SingleValueFormProperty>
  <property:SingleValueFormProperty rdf:ID="screenWidth">
    <rdfs:range rdf:resource="http://localhost/ontologies/Unit.owl#UnitValue"/>
    <rdfs:domain rdf:resource="#Display"/>
  </property:SingleValueFormProperty>
</rdf:RDF>

