<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:property="http://localhost/ontologies/StaticPropertyModel.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://localhost/ontologies/Unit.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://localhost/ontologies/Unit.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://localhost/ontologies/StaticPropertyModel.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="Basic">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Unit"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Length">
    <rdfs:subClassOf rdf:resource="#Basic"/>
  </owl:Class>
  <owl:Class rdf:ID="Temperature">
    <rdfs:subClassOf rdf:resource="#Basic"/>
  </owl:Class>
  <owl:Class rdf:ID="Time">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >time unit</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Basic"/>
  </owl:Class>
  <owl:Class rdf:ID="DerivedUnit">
    <rdfs:subClassOf rdf:resource="#Unit"/>
  </owl:Class>
  <owl:Class rdf:ID="Energy">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >energy unit</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DerivedUnit"/>
  </owl:Class>
  <owl:Class rdf:ID="Power">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >power unit</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DerivedUnit"/>
  </owl:Class>
  <owl:Class rdf:ID="MimeType">
    <rdfs:subClassOf rdf:resource="#Unit"/>
  </owl:Class>
  <owl:Class rdf:ID="Text">
    <rdfs:subClassOf rdf:resource="#MimeType"/>
  </owl:Class>
  <owl:Class rdf:ID="Video">
    <rdfs:subClassOf rdf:resource="#MimeType"/>
  </owl:Class>
  <owl:Class rdf:ID="UnitValue"/>
  <owl:Class rdf:ID="Audio">
    <rdfs:subClassOf rdf:resource="#MimeType"/>
  </owl:Class>
  <owl:Class rdf:ID="Currency">
    <rdfs:subClassOf rdf:resource="#Basic"/>
  </owl:Class>
  <owl:DatatypeProperty rdf:ID="value">
    <rdfs:domain rdf:resource="#UnitValue"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <Video rdf:ID="VideoMpeg"/>
  <Power rdf:ID="Watt"/>
  <Audio rdf:ID="AudioMpeg"/>
  <Temperature rdf:ID="Celsius"/>
  <Video rdf:ID="VideoOgg"/>
  <Audio rdf:ID="AudioOgg"/>
  <Audio rdf:ID="AudioMp3"/>
  <Length rdf:ID="Pixel"/>
  <Video rdf:ID="VideoMp4"/>
  <Energy rdf:ID="Joule"/>
  <Text rdf:ID="Plain"/>
  <Energy rdf:ID="WattHour"/>
  <Video rdf:ID="VideoAvi"/>
  <Text rdf:ID="Html"/>
  <Time rdf:ID="Milisecond"/>
  <Text rdf:ID="Css"/>
  <Length rdf:ID="Point"/>
  <Audio rdf:ID="AudioMp4"/>
  <Energy rdf:ID="KilowattHour"/>
  <Temperature rdf:ID="Fahrenheit"/>
  <Currency rdf:ID="EUR"/>
  <Power rdf:ID="MilliWatt"/>
  <Time rdf:ID="Second"/>
  <Currency rdf:ID="USD"/>
  <Text rdf:ID="Xml"/>
  <property:SingleValueAnnotationProperty rdf:ID="inUnit">
    <rdfs:domain rdf:resource="#UnitValue"/>
    <rdfs:range rdf:resource="#Unit"/>
  </property:SingleValueAnnotationProperty>
</rdf:RDF>

