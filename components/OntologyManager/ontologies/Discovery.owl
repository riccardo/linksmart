<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://localhost/ontologies/Discovery.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://localhost/ontologies/Discovery.owl">
  <owl:Ontology rdf:about=""/>
  <owl:Class rdf:ID="Sensor">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DiscoveryInfo"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="TellStick">
    <rdfs:subClassOf rdf:resource="#DiscoveryInfo"/>
  </owl:Class>
  <owl:Class rdf:ID="BlueToothService"/>
  <owl:Class rdf:ID="BlueTooth">
    <rdfs:subClassOf rdf:resource="#DiscoveryInfo"/>
  </owl:Class>
  <owl:Class rdf:ID="PhysicalDeviceSource">
    <rdfs:subClassOf rdf:resource="#DiscoveryInfo"/>
  </owl:Class>
  <owl:Class rdf:ID="RFIDTag">
    <rdfs:subClassOf rdf:resource="#DiscoveryInfo"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="btService">
    <rdfs:range rdf:resource="#BlueToothService"/>
    <rdfs:domain rdf:resource="#BlueTooth"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="sType">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Sensor"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="btClass">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#BlueTooth"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="btName">
    <rdfs:domain rdf:resource="#BlueTooth"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="btServiceName">
    <rdfs:domain rdf:resource="#BlueToothService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="tsName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#TellStick"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="sVendor">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Sensor"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="tsVendor">
    <rdfs:domain rdf:resource="#TellStick"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="btServiceType">
    <rdfs:domain rdf:resource="#BlueToothService"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="rfidVendor">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#RFIDTag"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="btType">
    <rdfs:domain rdf:resource="#BlueTooth"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
</rdf:RDF>

