<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rule="http://localhost/ontologies/PIDRuleModel.owl#"
    xmlns:property="http://localhost/ontologies/StaticPropertyModel.owl#"
    xmlns="http://localhost/ontologies/Application.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:device="http://localhost/ontologies/Device.owl#"
  xml:base="http://localhost/ontologies/Application.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://localhost/ontologies/PIDRuleModel.owl"/>
    <owl:imports rdf:resource="http://localhost/ontologies/StaticPropertyModel.owl"/>
    <owl:imports rdf:resource="http://localhost/ontologies/Device.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="LivingRoom">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Location"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="BedRoom">
    <rdfs:subClassOf rdf:resource="#Location"/>
  </owl:Class>
  <owl:Class rdf:ID="Person"/>
  <owl:Class rdf:ID="Resident">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>
  <owl:Class rdf:ID="SmartHome">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Application"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Hall">
    <rdfs:subClassOf rdf:resource="#Location"/>
  </owl:Class>
  <owl:Class rdf:ID="MaintananceCrew">
    <rdfs:subClassOf rdf:resource="#Person"/>
  </owl:Class>
  <owl:DatatypeProperty rdf:ID="applicationName">
    <rdfs:domain rdf:resource="#Application"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:ID="locatedIn">
    <rdfs:domain rdf:resource="http://localhost/ontologies/Device.owl#Device"/>
    <rdfs:range rdf:resource="#Location"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="ownedBy">
    <rdfs:domain rdf:resource="http://localhost/ontologies/Device.owl#Device"/>
    <rdfs:range rdf:resource="#Person"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="personName">
    <rdfs:domain rdf:resource="#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="locationName">
    <rdfs:domain rdf:resource="#Location"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <property:MultiValueFormProperty rdf:ID="hasLocation">
    <rdfs:domain rdf:resource="#Application"/>
    <rdfs:range rdf:resource="#Location"/>
  </property:MultiValueFormProperty>
  <property:MultiValueFormProperty rdf:ID="hasUser">
    <rdfs:range rdf:resource="#Person"/>
    <rdfs:domain rdf:resource="#Application"/>
  </property:MultiValueFormProperty>
  <property:MultiValueFormProperty rdf:ID="ownsDevice">
    <rdfs:range rdf:resource="http://localhost/ontologies/PIDRuleModel.owl#DevicePID"/>
    <rdfs:domain rdf:resource="#Person"/>
    <rule:generatesInverseProperty rdf:resource="#ownedBy"/>
  </property:MultiValueFormProperty>
  <property:MultiValueFormProperty rdf:ID="containsDevice">
    <rdfs:range rdf:resource="http://localhost/ontologies/PIDRuleModel.owl#DevicePID"/>
    <rdfs:domain rdf:resource="#Location"/>
    <rule:generatesInverseProperty rdf:resource="#locatedIn"/>
  </property:MultiValueFormProperty>
</rdf:RDF>

